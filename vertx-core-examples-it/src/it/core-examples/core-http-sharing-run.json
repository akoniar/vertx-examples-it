{
  "name": "http-sharing",

  "tags": ["http", "sharing", "ssl", "core"],

  "executions": {

    "java" : {
      "directory" : "${base}/src/main/js/io/vertx/example/core/http/sharing",
      "command" : "java -jar ${jarFile} run io.vertx.example.core.http.sharing.Server -cp ${base}/target/classes:.",
      "client-command" : "java -jar ${jarFile} run io.vertx.example.core.http.sharing.Client -cp ${base}/target/classes:."
    }
  },

  "grace-text": "Succeeded in deploying verticle",
  "client-execute-until": "Hello from io.vertx.example.core.http.sharing.HttpServerVerticle"
}