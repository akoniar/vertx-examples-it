{
  "name": "net-echo-ssl",

  "tags": ["net", "ssl", "echo", "core"],

  "executions": {

    "java" : {
      "directory" : "${base}/src/main/js/io/vertx/example/core/net/echossl",
      "command" : "java -jar ${jarFile} run io.vertx.example.core.net.echossl.Server -cp ${base}/target/classes:.",
      "client-command" : "java -jar ${jarFile} run io.vertx.example.core.net.echossl.Client -cp ${base}/target/classes:."
    }
  },

  "post-check": "core-net-echo-check.groovy",
  "grace-text": "Succeeded in deploying verticle",
  "client-execute-until" : "client receiving"
}