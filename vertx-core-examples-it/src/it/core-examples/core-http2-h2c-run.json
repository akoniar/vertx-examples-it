{
  "name": "http2-h2c",
  "tags": [
    "http2",
    "core",
    "h2c"
  ],
  "libraries": [
    "${dependencies}/netty-tcnative-boringssl-static"
  ],
  "executions": {
    "java": {
      "directory": "${base}/src/main/java/io/vertx/example/core/http2/h2c",
      "command": "java -jar ${jarFile} run io.vertx.example.core.http2.h2c.Server -cp ${base}/target/classes:.",
      "client-command": "java -jar ${jarFile} run io.vertx.example.core.http2.h2c.Client  -cp ${base}/target/classes:."
    }
  },
  "post-check": "core-http2-simple-check.groovy",
  "grace-text": "Succeeded in deploying verticle",
  "client-execute-until": "Got data"
}