{
  "name": "http2-push",
  "tags": [
    "http2",
    "core",
    "push"
  ],
  "libraries": [
    "${dependencies}/netty-tcnative-boringssl-static"
  ],
  "executions": {
    "java": {
      "directory": "${base}/src/main/java/io/vertx/example/core/http2/push",
      "command": "java -jar ${jarFile} run io.vertx.example.core.http2.push.Server -cp ${base}/target/classes:.",
      "client-command": "java -jar ${jarFile} run io.vertx.example.core.http2.push.Client  -cp ${base}/target/classes:."
    }
  },
  "post-check": "core-http2-push-check.groovy",
  "grace-text": "Server started",
  "client-execute-until": "<h1>Hello World</h1>"
}