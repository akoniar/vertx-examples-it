{
  "name": "http2-custom-frames",
  "tags": [
    "http2",
    "core",
    "frames"
  ],
  "libraries": [
    "${dependencies}/netty-tcnative-boringssl-static"
  ],
  "executions": {
    "java": {
      "directory": "${base}/src/main/java/io/vertx/example/core/http2/customframes",
      "command": "java -jar ${jarFile} run io.vertx.example.core.http2.customframes.Server -cp ${base}/target/classes:.",
      "client-command": "java -jar ${jarFile} run io.vertx.example.core.http2.customframes.Client -cp ${base}/target/classes:."
    }
  },
  "post-check": "core-http2-customframes-check.groovy",
  "grace-text": "Succeeded in deploying verticle",
  "client-execute-until": "Got frame from server pong"
}